{
  "version": 2,
  "builds": [
    {
      "src": "Щербина_index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/style/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      },
      "dest": "/style/$1"
    },
    {
      "src": "/img/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/img/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Щербина_index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ]
}