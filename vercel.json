{
  "version": 2,
  "builds": [
    {
      "src": "Щербина_index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/style/(.*)",    
      "dest": "/style/$1"
    },
    {
      "src": "/img/(.*)",      
      "dest": "/img/$1"
    },
    {
      "src": "/(.*)",          
      "dest": "/Щербина_index.html"
    }
  ]
}